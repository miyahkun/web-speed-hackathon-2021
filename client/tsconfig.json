{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es2020",
      "DOM"
    ],
    "outDir": "./build",
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "allowJs": true
  }
}
